{
  "extends": [
    "prettier",
    "airbnb-base",
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "function-paren-newline": 0,
    "implicit-arrow-linebreak": 0,
    "consistent-return": 0,
    "object-curly-newline": 0,
    "operator-linebreak": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "no-unused-vars": 0,
    "no-console": 0,
    "indent": 0,
    "linebreak-style": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100,
        "tabWidth": 4,
        "semi": true
      }
    ]
  },
  "plugins": ["prettier", "react", "import", "jsx-a11y"]
}
